<%-include ("_header")-%>

<div class="artigo">
<h1>Primeira Pagina</h1>
<form action=" " method="post" class="home" enctype="multipart/form-data">
    <br>
    <img id="frame" src="" width="100px" height="100px" alt="carregando..."/>
    <br>
    <input type="file" name="arquivo" onchange="preview()" required>   
    <input type="reset" value="Limpar" onclick="document.getElementById('frame').src=''" />
    <input type="text" name="titulo" placeholder="Titulo" required/>
     <textarea name="mensagem" rows="4" cols="20" placeholder="Mensagem" required></textarea>
    <input type="submit" value="Enviar">
</form>
</div>

<%-include ("_footer")-%>